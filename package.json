{
  "name": "clis",
  "version": "1.0.0",
  "description": "A collection of CLI utilities",
  "scripts": {
    "dev": "run-p watch:test watch:lint",
    "lint": "eslint ./",
    "watch:test": "nodemon --exec \"npm test || true\"",
    "watch:lint": "nodemon --exec \"npm run lint || true\"",
    "cli:echo": "node echo.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CrabDude/nodejs-clis.git"
  },
  "author": "Adam Crabtree",
  "license": "MIT",
  "bin": {
    "cli-echo": "./echo.js",
    "cli-touch": "./touch.js",
    "cli-ls": "./ls.js",
    "cli-mkdir": "./mkdir.js",
    "cli-rm": "./rm.js",
    "cli-cat": "./cat.js",
    "cli-ln": "./ln.js",
    "cli-grep": "./grep.js"
  },
  "bugs": {
    "url": "https://github.com/enixdark/Project_001_CLI_Utilities/issues"
  },
  "homepage": "https://github.com/enixdark/Project_001_CLI_Utilities#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-nodev6": "^1.0.0",
    "babel-register": "^6.14.0",
    "glob-fs": "^0.1.6",
    "glob-stream": "^5.3.5",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "pn": "^1.0.0",
    "ramda": "^0.22.1",
    "songbird": "^1.0.4",
    "trycatch": "^1.5.21",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "eslint-config-noderiety": "^1.1.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  }
}
